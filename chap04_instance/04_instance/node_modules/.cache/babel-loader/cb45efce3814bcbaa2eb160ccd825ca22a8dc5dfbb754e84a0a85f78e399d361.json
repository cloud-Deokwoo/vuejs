{"ast":null,"code":"import { ref, watchEffect } from 'vue';\nexport default {\n  setup() {\n    const count = ref(0);\n    const increment = () => {\n      count.value++;\n    };\n    watchEffect(() => {\n      console.log(`Count is now: ${count.value}`);\n    });\n    return {\n      count,\n      increment\n    };\n  }\n};","map":{"version":3,"names":["ref","watchEffect","setup","count","increment","value","console","log"],"sources":["D:\\gitRepository\\sunglee0517\\vuejs\\chap04_instance\\04_instance\\src\\components\\WatchEffect.vue"],"sourcesContent":["<template>\r\n    <div>\r\n      <h2>WatchEffect Example</h2>\r\n      <p>{{ count }}</p>\r\n      <button @click=\"increment\">Increment</button>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import { ref, watchEffect } from 'vue';\r\n  \r\n  export default {\r\n    setup() {\r\n      const count = ref(0);\r\n      const increment = () => {\r\n        count.value++;\r\n      };\r\n  \r\n      watchEffect(() => {\r\n        console.log(`Count is now: ${count.value}`);\r\n      });\r\n  \r\n      return {\r\n        count,\r\n        increment\r\n      };\r\n    }\r\n  };\r\n  </script>"],"mappings":"AASE,SAASA,GAAG,EAAEC,WAAU,QAAS,KAAK;AAEtC,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIH,GAAG,CAAC,CAAC,CAAC;IACpB,MAAMI,SAAQ,GAAIA,CAAA,KAAM;MACtBD,KAAK,CAACE,KAAK,EAAE;IACf,CAAC;IAEDJ,WAAW,CAAC,MAAM;MAChBK,OAAO,CAACC,GAAG,CAAC,iBAAiBJ,KAAK,CAACE,KAAK,EAAE,CAAC;IAC7C,CAAC,CAAC;IAEF,OAAO;MACLF,KAAK;MACLC;IACF,CAAC;EACH;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}